{
  "name": "@govex/futarchy-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for Govex Futarchy Protocol on Sui",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./sdk": {
      "types": "./dist/esm/sdk/index.d.ts",
      "import": "./dist/esm/sdk/index.js",
      "require": "./dist/cjs/sdk/index.js"
    },
    "./workflows": {
      "types": "./dist/esm/workflows/index.d.ts",
      "import": "./dist/esm/workflows/index.js",
      "require": "./dist/cjs/workflows/index.js"
    },
    "./protocol": {
      "types": "./dist/esm/protocol/index.d.ts",
      "import": "./dist/esm/protocol/index.js",
      "require": "./dist/cjs/protocol/index.js"
    },
    "./config": {
      "types": "./dist/esm/config/index.d.ts",
      "import": "./dist/esm/config/index.js",
      "require": "./dist/cjs/config/index.js"
    },
    "./types": {
      "types": "./dist/esm/types/index.d.ts",
      "import": "./dist/esm/types/index.js",
      "require": "./dist/cjs/types/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govex-dao/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/govex-dao/sdk/issues"
  },
  "homepage": "https://github.com/govex-dao/sdk#readme",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "protocol-setup": "tsx scripts/protocol-setup.ts",
    "protocol-init": "tsx scripts/protocol-init.ts",
    "create-test-coins": "tsx scripts/create-test-coins.ts",
    "create-dao-direct": "tsx scripts/create-dao-direct.ts",
    "deploy-conditional-coins": "tsx scripts/deploy-conditional-coins.ts",
    "generate-conditional-coins": "tsx scripts/generate-conditional-coins.ts",
    "test:launchpad": "tsx scripts/launchpad-e2e.ts",
    "test:launchpad-failure": "tsx scripts/launchpad-e2e.ts failure",
    "test:proposal-with-swaps": "tsx scripts/proposal-e2e-with-swaps.ts",
    "test:proposal-balance-swaps": "tsx scripts/proposal-e2e-balance-swaps.ts",
    "test:arbitrage-challenge": "tsx scripts/proposal-e2e-arbitrage-challenge.ts",
    "test:reject-wins": "tsx scripts/test-reject-wins.ts",
    "test:memo-action": "tsx scripts/test-memo-action.ts",
    "test:sponsorship": "tsx scripts/test-sponsorship.ts",
    "test:multi-outcome": "tsx scripts/test-multi-outcome.ts",
    "test:execution-timeout": "tsx scripts/test-execution-timeout.ts",
    "test:execution-timeout-quick": "tsx scripts/test-execution-timeout.ts --skip-wait",
    "launchpad-e2e-two-outcome": "tsx scripts/launchpad-e2e.ts",
    "validate-deployments": "tsx scripts/validate-deployments.ts",
    "validate-deployments:verbose": "tsx scripts/validate-deployments.ts --verbose"
  },
  "keywords": [
    "sui",
    "futarchy",
    "governance",
    "dao",
    "prediction-markets",
    "typescript",
    "sdk"
  ],
  "author": "Govex",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.14.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
